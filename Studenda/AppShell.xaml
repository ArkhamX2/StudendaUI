<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Studenda.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Studenda.View"
    BackgroundColor="{DynamicResource PanelColor}"
    HeightRequest="1034"
    Shell.FlyoutBehavior="Disabled"
    WidthRequest="750">

    <ShellContent
        Title="Studenda"
        ContentTemplate="{DataTemplate local:LogInView}"
        Route="LoginInView" />
    <ShellContent
        Title="Studenda"
        ContentTemplate="{DataTemplate local:ScheduleView}"
        Route="ScheduleView" />
    <ShellContent
        Title="Studenda"
        ContentTemplate="{DataTemplate local:ProfileView}"
        Route="ProfileView" />

</Shell>
