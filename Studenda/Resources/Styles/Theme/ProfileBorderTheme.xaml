<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Studenda.Resources.Styles.Theme.ProfileBorderTheme">
    <Style TargetType="{x:Type Border}" x:Key="ProfileBorderTheme">
        <Setter Property="HorizontalOptions" Value="Start"/>
        <Setter Property="VerticalOptions" Value="Start"/>
        <Setter Property="HeightRequest" Value="26"/>
        <Setter Property="WidthRequest" Value="200"/>
        <Setter Property="BackgroundColor" Value="White"/>
        <Setter Property="StrokeShape" Value="RoundRectangle 5"/>
    </Style>

</ResourceDictionary>